  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch A4-500",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/x86_64/debug/bin/election",
      "args": ["-w", "1024x1280"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [{ "name": "LC_ALL", "value": "it_IT.UTF-8" }],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch A4-600 GTK debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/x86_64/debug/bin/election",
      "args": ["-w", "1920x1080"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build",
      "environment": [
        { "name": "LC_ALL", "value": "es_PY" },
        { "name": "GTK_DEBUG", "value": "interactive" },
        { "name": "GOBJECT_DEBUG", "value": "instance-count" }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch A4-200",
      "type": "cppdbg",
